[
  {
    "defaults": {
      "link": {
        "create": true
      }
    }
  },
  {
    "clean": ["~/.config/nvim"]
  },
  {
    "link": {
      "~/.config/nvim/init.vim": "init.vim"
    }
  },
  {
    "shell": [
      {
        "command":
          "nix-env -iA neovim python27Packages.neovim python36Packages.neovim vim-vint -f '<nixpkgs>'",
        "description": "Installing neovim"
      }
    ]
  }
]
