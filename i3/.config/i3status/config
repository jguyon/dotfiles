general {
        output_format = "i3bar"
        interval = 5
        colors = true
        color_good = "#A3BE8C"
        color_degraded = "#EBCB8B"
        color_bad = "#BF616A"
}

order += "volume master"
order += "ethernet enp2s0"
order += "time"

volume master {
        format = "VOL %volume"
        format_muted = "VOL muted"
}

ethernet enp2s0 {
        format_up = "ETH up"
        format_down = "ETH down"
}

time {
        format = "%A %d %b %H:%M"
}
